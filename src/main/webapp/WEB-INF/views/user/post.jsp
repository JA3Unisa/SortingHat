<%@ page import="Model.Discussione.Discussione" %>
<%@ page import="Model.Risposta.Risposta" %>
<%@ page import="java.util.List" %>
<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <jsp:include page="../partials/headS.jsp">
        <jsp:param name="userStyles" value="main,background,post"/>
        <jsp:param name="userScripts" value="mobileMenu"/>
        <jsp:param name="title" value="SortingHat - Homepage"/>
    </jsp:include>
</head>
<body>
<jsp:include page="/WEB-INF/views/partials/background.jsp"></jsp:include>
<jsp:include page="/WEB-INF/views/partials/menuDesktop.jsp"></jsp:include>
<jsp:include page="/WEB-INF/views/partials/menuMobile.jsp"></jsp:include>
<div id="main">
    <%
        Discussione discussione = (Discussione) request.getAttribute("discussione");
        List<Risposta> risposte = (List<Risposta>) request.getAttribute("risposte");

        String nome = discussione.getUtente().getNome() + " " + discussione.getUtente().getCognome();


    %>
    <div id="post">
        <div id="utente_post">
            <img src="data:image/jpeg;base64," alt="" class="profile_pic">
            <p class="nome_utente"><%= nome%></p>
        </div>
        <div id="title_post">
            <p id="post_title_text"><%= discussione.getTitolo()%></p>
            <p id="post_date"><%= discussione.getDataOra()%></p>
        </div>
        <p id="corpo_post"><%= discussione.getCorpo()%></p>
        <div id="post_files">
            <a class="file" target="_blank" href="https://p.kindpng.com/picc/s/24-248253_user-profile-default-image-png-clipart-png-download.png">
                <i class="fas fa-external-link-alt"></i>
                <p class="nome_file">prova.txt</p>
            </a>

        </div>
    </div>
    <div id="commenti">
        <%
            for (Risposta r: risposte) {
                String nomeR = r.getUtente().getNome() + " " + r.getUtente().getCognome();

        %>
        <div class="commento">
            <div class="utente">
                <img src="data:image/jpeg;base64," alt="" class="profile_pic">
                <p class="nome_utente"><%= nomeR%></p>
            </div>
            <p class="corpo_commento"><%= r.getCorpo()%></p>
            <p class="ora_commento"><%= r.getdataOra()%></p>
        </div>
        <%}%>
    </div>
    <form id="commenta" action="../risposte/createPost" method="post">
        <input hidden name="idDiscussione" value="<%= discussione.getIdDiscussione()%>">
        <textarea name="commento" rows="2" cols="50"></textarea>
        <button type="submit" class="btn btn-success"><i class="far fa-paper-plane"></i>
        </button>
    </form>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>
    function openMenuMobile(){
        $( "#main_menu_mobile" ).removeClass( "menuMobileChiuso" ).addClass( "menuMobileAperto" );
    }

    function closeMenuMobile(){
        $( "#main_menu_mobile" ).removeClass( "menuMobileAperto" ).addClass( "menuMobileChiuso" );
    }
</script>
</body>
</html>
